generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model items {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  URL         String
  description String
  name        String
  price       Int
}
